
.header{
  height: 100px;
  overflow: hidden;
  position: relative;
  background: #fff;
}
.header .address{
  padding:25px 0;
}
.header .user{
  padding:10px 15px;
}
.header .user:after{
  content: "";
  display: block;
  clear: both;
}
.header .user .name{
  font-size: 16px;
  float: left;
  color:#333;
}
.header .user .tel{
  font-size: 16px;
  float: right;
  color:#333;
}
.header .ress{
  padding:0 15px 15px 15px;
  font-size:12px;
  color:#999;
}
.header .distribution{
  position: absolute;
  top: 0;
  left:0;
  font-size:12px;
  background: #6fb1f9;
  padding:3px 10px;
  color: #fff;
}
.xian{
  border-bottom:2px dashed #ff533b;
  position: absolute;
  width:100%;
  bottom: 0;
  left:0;
}
.xian2{
  background: #fff;
}
.toAddress{
    color: 9e9e9e;
    background: #fff;
    font-size: 18px;
    padding: 15px;
}
.box{
    margin-top:10px;
}
.box .title{
  font-size: 12px;
  color: #9e9e9e;
  padding:10px 15px;
}

.box .imgtu{
  margin-top:5px;
  padding:3px 15px;
  background: #fff;
  border-top:1px solid #eee;
  border-bottom:1px solid #eee;
  height: 30px;
}

.box .imgtu image{
  width:30px;
  height: 28px;
  float: left;
}
.box .imgtu text{
  line-height: 30px;
  font-size: 12px;
  float: left;
  padding-left:10px;
  color: #999;
}
.box .list {
  background: #fff;
  padding:10px 15px;
  margin-top:5px;
}
.box .list .item{
  font-size: 14px;
  line-height: 20px;
  color: #999;
}
.box .item:after{
  content: "";
  display: block;
  clear: both;
}
.box .item .left{
  float: left;
}
.box .item .right{
  float: right;
}
.box .fot{
  font-size: 14px;
  color: #333;
  margin-top:5px;
  border-top:1px solid #eee;
  padding-top:8px;
}
.box .fot .left{
  float: left;
}
.box .fot .right{
  float: right;
}
.box .fot:after{
  content: "";
  display: block;
  clear: both;
}
.number {
  width:110px;
  height: 50px;
  position: absolute;
  right:0;
  top:25px;
}
.number:after{
  content:"";
  display: block;
  clear: both;
}
.number .plus{
  width:30px;
  height: 30px;
  line-height: 35px;
  text-align: center;
  color:#fff;
  background: #ff533b;
  border-radius: 50%;
  font-size: 20px;
  float: left;
  margin-top:10px;
}
.number .sum{
  float: left;
  width:40px;
  line-height: 50px;
  font-size: 14px;
  text-align: center;
}
.number .jiax{
  margin-right:15px;
}

.footer{
}
.footer .right{
  float: right;
}
.footer .left{
  float: left;
}
.footer .left {
  line-height: 98rpx;
  font-size: 14px;
  padding-left:15px;
  color:#666;
}
.footer .left .span {
  color:#ff533b;
  padding-left:5px;
}
.footer .right button{
  height: 98rpx;
  background: #F5B223;
  border:none;
  border-radius: 0;
  color:#fff;
  line-height:98rpx;
  padding:0 50px;
}
scroll-view{
  margin-bottom: 50px;
}
.list{
  border-top:1px solid #eee;
  margin-bottom:10px;
}
.list .item {
  background: #fff;
  padding: 10px 15px;
  height: 80px;
  border-bottom: 1px solid #eee;
  position: relative;
}

.list .item .image {
  width:90px;
  height: 80px;
  position: absolute;
  left:15px;
  top:10px;
}
.list .item .image image{
  width:100%;
  height: 100%;
}
.list .item .content {
  padding-left:100px;
}
.list .item .content .h1{
  display: block;
  font-size: 30rpx;
  color:#666;
  line-height: 40rpx;
  margin-bottom: 15px;
  text-overflow: ellipsis;
  overflow: hidden;  
  white-space:nowrap; 
  height: 40rpx;
}
.list .item .content .p{
  display: block;
  font-size: 12px;
  color:#999;
  line-height: 25px;
}
.list .item .content .myp{
  font-size:14px;
  color: #ff533b;
}
.post_fee,.fixinline {
    padding: 12px 0;
    position: relative;
    border-bottom: solid 1px #e9e9e9;
    font-size: 14px;
    background-color: #fff;
}
.msettlement_goods_con .pdl10{
    padding-left: 10px;
    padding-right: 10px;
}


.address{
    height:194rpx;
    background: url('http://app.shizhencaiyuan.com/pintuan/img/address.png') no-repeat;
    background-size: cover;
    margin-bottom: 20rpx;
}
.address .address-item-top{
    width: 90%;
    padding: 30rpx 0 30rpx 30rpx;
    box-sizing: border-box;
}
.address .address-item-top view:first-child{
    padding-bottom: 30rpx;
}
.address .address-item-right{
    width: 10%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.address .address-item-right image{
    width: 26rpx;
    height: 48rpx;
}
.goods-info{
    padding: 30rpx;
    background: white;
    display: flex;
    margin-bottom: 20rpx;
}
.goods-info image{
  width:70px;
  height:70px;
  display:inline-block;
  overflow:hidden;
}
.goods-info .goods-info-handler{
    padding-left: 20rpx;
    width: 80%;
}
.goods-info .goods-info-handler .goods-info-name{
    line-height: 1.2;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}
.goods-info .goods-info-handler .goods-info-price{
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 30rpx;
}
.goods-info .goods-info-handler .goods-info-price .calc{
    width: 160rpx;
    display: flex;
    justify-content: space-around;
    border: 1px solid #ddd;
    align-items: center;
    /*padding: 0 20rpx;*/
    box-sizing: border-box;
}
.goods-info .goods-info-handler .goods-info-price .calc text{
    display: block;
    height: 100%;
    width: 160rpx;
    text-align: center;
    line-height: 60rpx;
}
.coupon{
    padding: 30rpx;
    background: white;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 20rpx;
}
.coupon .show-coupon{
    display: flex;
    align-items: center;
}
.coupon .show-coupon image{
    width: 22rpx;
    height: 44rpx;
    padding-left: 10rpx;
}